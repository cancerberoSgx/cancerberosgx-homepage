    "offline-plugin": "^5.0.7",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.11.0",
    "react-jss": "^10.0.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.1.0",
    "@types/classnames": "^2.2.9",
